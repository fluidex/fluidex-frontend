{
  "extends": ["react-app", "prettier"],
  "globals": {
    "Datafeeds": true,
    "TradingView": true
  },
  "rules": {
    "no-template-curly-in-string": "off",
    "no-useless-escape": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "no-extend-native": "off",
    "no-sequences": "off",
    "react-hooks/exhaustive-deps": "off"
  }
}
